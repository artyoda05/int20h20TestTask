<div class="center">
    <div class="lyric_search">
        <h1>Search song by lyrics</h1>
        <form action="" method="GET">
            <div class="lyric_search_input">
                <input class="lyrics_input" type="search" name="lyrics">
                <input class="_button" class="lyrics_input" type="submit" value="Search">
            </div>
        </form>
    </div>
</div>
<div class="song_results">
    <h2>Results:</h2>
    <ul class="songs_info">
        {{#each data}}
        <div class="song">
            <p>Title: {{title}}</p>
            <p>Artist: {{artist}}</p>
            <p>Lyrics<br> {{lyrics}}</p>
            <p>
                {{#each deezerData}}
                    <iframe scrolling="no" frameborder="0" allowTransparency="true" src="https://www.deezer.com/plugins/player?format=classic&autoplay=false&width=500&height=200&color=ff0000&layout=dark&size=medium&type=tracks&id={{id}}&app_id=1" width="500" height="200"></iframe>
                {{/each}}
            </p>
        </div>
        {{/each}}
    </ul>
</div>